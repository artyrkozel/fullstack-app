{
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "prepare": "husky install",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "turbo": "^2.3.3",
    "eslint-config-custom": "*"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "yarn@1.22.22",
  "lint-staged": {
    "apps/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  }
}
